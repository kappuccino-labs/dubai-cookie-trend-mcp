    "summary": "npx tsx src/consume-summary.ts"
  },
  "keywords": ["naver", "kafka", "food-trend", "mcp"],
  "author": "Kim SangJin (EccentricJin)",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.4",
    "kafkajs": "^2.2.4"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
